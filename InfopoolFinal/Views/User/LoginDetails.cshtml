
@{
    ViewBag.Title = "LoginDetails";
}
    <body style="background:url(../images/pic18.jpg);background-size:cover">
        <div style="background-color:white;padding:2%;margin-top:2%">

            <h2>Login Details</h2>

           
            <table table class="table table-bordered table-striped" style="background-color:white;">
                <tr>
                    <th style="border: 2px solid #2C3E50;font-weight:bold">
                        Session ID
                    </th>
                    <th style="border: 2px solid #2C3E50;font-weight:bold">
                        Session Start
                    </th>
                    <th style="border: 2px solid #2C3E50;font-weight:bold">
                        Session End
                    </th>
                </tr>
                @for (int i = 0; i < Model.Rows.Count; i++)
                {
                    <tr>

                        <td style="border: 1px solid #2C3E50;">@(i + 1)</td>
                        <td style="border: 1px solid #2C3E50;">@Model.Rows[i][1]</td>
                        <td style="border: 1px solid #2C3E50;">@Model.Rows[i][2]</td>
                    </tr>
                }


            </table>
        </div>
    </body>